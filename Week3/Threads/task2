package thread;

public class K_task1_Thread2 {
 public void run() {
	System.out.println("From Thread2"); 
	System.out.println("From Thread2"); 
	System.out.println(10/0); 
 }
}
class Thread{
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		K_task1_Thread2 t=new K_task1_Thread2();
		t.run();
	}


}
